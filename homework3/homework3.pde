void setup(){
  size(800,800);
  background(0);
}

void draw(){
  strokeWeight(8);
  stroke(0);
  
  fill(0);//黑色
  rect(100,300,200,100);
  rect(700,400,100,100);
  
  fill(255);//白色
  rect(400,0,200,100);
  rect(600,0,100,100);
  rect(700,0,100,300);
  rect(0,100,100,300);
  rect(500,300,100,100);
  rect(700,300,100,100);
  rect(400,300,100,100);
  rect(0,400,100,200);
  rect(400,400,100,300);
  rect(0,700,100,100);
  rect(100,700,400,100);
  rect(600,500,200,300);
  
  fill(#F52525);//红色
  rect(400,100,200,200);
  rect(300,300,100,100);
  rect(600,400,100,100);
  rect(0,600,100,100);
  
  fill(#F5EB25);//黄色
  rect(100,0,300,100);
  rect(600,100,100,100);
  rect(600,200,100,200);
  rect(100,400,300,300);
  
  fill(#3E7AC1);//蓝色
  rect(0,0,100,100);
  rect(100,100,300,200);
  rect(500,400,100,400);
}
